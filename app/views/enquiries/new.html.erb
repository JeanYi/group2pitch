<h3>Enquiry Form</h3>

<%= form_for @enquiry , url:enquiries_path do |f| %>

	<p>
		<%=f.label "subject matter 1" %><br>
		<%=f.text_field :subject_one %>		
	</p>

	<p>
		<%=f.label "subject matter 2" %><br>
		<%=f.text_field :subject_two %>		
	</p>

	<p>
		<%=f.label "budget " %><br>
		<%=f.number_field :budget %>		
	</p>

	<p>
		<%=f.label "start date" %><br>
		<%=f.date_field :start_date %>		
	</p>

	<p>
		<%=f.label "end date" %><br>
		<%=f.date_field :end_date %>		
	</p>

	<p>
		<%=f.label "venue" %><br>
		<%=f.text_field :venue %>		
	</p>

	<%= f.submit %>

<% end %>